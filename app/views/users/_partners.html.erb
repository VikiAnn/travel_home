<div class="row col-md-12" style="margin-top: 20px;">
  <%= render partial: "partners/new" %>
  <h3>Your current partners</h3>
  <a href="#" class="btn btn-default" data-toggle="modal" data-target="#partnerModal">
    Add a partner
  </a>
  <% if current_user.partners.any? %>
    <% current_user.partners.each do |partner| %>
      <div class="listing">
        <%= partner.name %><br>
        <%= partner.email_address %>
        <%= link_to "Remove Partner", partner_path(partner.id), method: :delete, class: "btn btn-default" %>
      </div>
    <% end %>
  <% end %>
</div>
