<div class="col-md-12 listing">
    <div class="filter-head">
      Refine your search
    </div>
    <div>
      <%= form_tag properties_path, :method => 'get', :id => "properties_search" do %>
        <div class="form-group">
          <%= text_field_tag :search, params[:search], placeholder: "City", class: "form-control modal-form" %>
        </div>
        <div class="form-group">
          <input id="moneySlide" name="moneySlide" type="text" value="" data-slider-min="0" data-slider-max="1000" data-slider-step="5" data-slider-value="[0,1000]"/>
          $<span id="lowVal">0</span> <span class="highVal">$<span id="highVal">1000</span></span>
        </div>
        <div class="form-group">
          <%= select_tag :category, options_from_collection_for_select(Category.all, 'id', 'name'), multiple: true, class: "selectpicker form-control"%>
        </div>
      <% end %>
    </div>
</div>


