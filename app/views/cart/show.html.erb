<h1>HI</h1>
<% if @cart.empty? %>
  <h3>Don't you want to go somewhere?</h3>
<% else %>
    It appears you want to go to
    <h4><%= @cart.property.title %></h4>
    from
    <h4><%= @cart.start_date %></h4>
    to
    <h4><%= @cart.end_date %></h4>
  <%= button_to "Don't go to #{@cart.property.title}", cart_path, method: :delete %>
<% end %>

<%= form_tag "/properties/5/add_to_cart", method: :post do %>
  <%= date_field_tag "property[start_date]" %>
  <%= date_field_tag "property[end_date]" %>
  <%= submit_tag "Request reservation mofo" %>
<% end %>
